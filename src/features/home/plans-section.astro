---
import PlanTile from "./plan-tile.astro";
import { appConfig } from "@/core/config";
---

<section
  class="fluid full-section flex flex-col text-center justify-center items-center"
  aria-labelledby="plans-title"
  id={appConfig.plansSection.id}
>
  <h2 class="text-h2 font-500 mb-16 max-w-2xl" id="plans-title">
    Wybierz plan nauki i umów się na spotkanie oraz otrzymaj materiały
  </h2>

  <div class="relative flex flex-wrap justify-center gap-4">
    <PlanTile
      button={{
        label: "Umów spotkanie",
        href: appConfig.contactSection.calendarLink,
        title: "Umów spotkanie konsultacyjne w ramach Szybkiej Pomocy",
      }}
      price={{
        id: "quick-help-price",
        label: `${appConfig.plansSection.plans.quickCall.cost} ${appConfig.plansSection.plans.quickCall.currency}`,
        description: "Bonus (2k tokenów do 4markdown.com)",
      }}
      heading={{
        title: {
          label: appConfig.plansSection.plans.quickCall.label,
          id: "quick-help-title",
        },
        description: {
          label: "Masz problem? Rozwiążemy go razem",
          id: "quick-help-description",
        },
      }}
      benefits={{
        title: `Korzyści planu ${appConfig.plansSection.plans.quickCall.label}`,
        items: [
          "Wsparcie przy dowolnym problemie związanym z programowaniem lub IT",
          "Elastyczne rozliczenie godzinowe - płacisz za rzeczywisty czas konsultacji",
          "Potrzebujesz zrozumieć zagadnienie? Śmiało! To plan idealny dla Ciebie",
          "30 minut na wycofanie się (jeżeli spotkanie nie spełnia Twoich oczekiwań)",
        ],
      }}
    >
      <Fragment slot="flare">
        <div class="radial-flare size-30 bottom-0 left-0" aria-hidden="true">
        </div>
      </Fragment>
      <Fragment slot="icon">
        <svg
          width="68"
          height="66"
          viewBox="0 0 68 66"
          fill="none"
          id="quick-help-icon"
          xmlns="http://www.w3.org/2000/svg"
        >
          <title id="quick-help-icon">Ikona konsultacji</title>
          <g filter="url(#filter0_d_211_729)">
