    <PlanTile
      button={{
        label: "Dogadajmy się",
        href: appConfig.contactSection.linkedInLink,
        title: "Stwórz własny plan",
      }}
      price={{
        id: "own-plan-price",
        label: appConfig.plansSection.plans.custom.cost,
        description: "Bonusy ustalimy prywatnie",
      }}
      heading={{
        title: {
          label: appConfig.plansSection.plans.custom.label,
          id: "own-plan-title",
        },
        description: {
          label: "Podejście w 100% dopasowane do Twoich preferencji",
          id: "own-plan-description",
        },
      }}
      benefits={{
        title: "Korzyści własnego planu",
        items: [
          "Na początek poznajmy się",
          "Określimy reguły gry, cenę oraz czas",
          "Wybierzemy stack, projekt oraz stworzymy odpowiedni program pod Ciebie",
          "Osiągniemy dowolny cel"
        ],
      }}
    >
      <Fragment slot="flare">
        <div class="radial-flare size-30 top-0 left-0 right-0 m-auto bottom-0" aria-hidden="true">
        </div>
      </Fragment>
      <Fragment slot="icon">
        <svg
          width="64"
          height="64"
          viewBox="0 0 64 64"
          fill="none"
          aria-labelledby="own-plan-icon"
          xmlns="http://www.w3.org/2000/svg"
