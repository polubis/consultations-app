      price={{
        id: "senior-price",
        label: `${appConfig.plansSection.plans.seniorInYear.cost} ${appConfig.plansSection.plans.seniorInYear.currency}`,
        description: "Bonus (10k tokenów do 4markdown.com)",
      }}
      heading={{
        title: {
          label: appConfig.plansSection.plans.seniorInYear.label,
          id: "senior-title",
        },
        description: {
          label: "W rok osiągniemy dowolny cel",
          id: "senior-description",
        },
      }}
      benefits={{
        title: `Korzyści planu ${appConfig.plansSection.plans.seniorInYear.label}`,
        items: [
          "Na początek Mocked Technical Interview (2h) w cenie",
          "Określamy Twój poziom, stack oraz kierunek rozwoju",
          "Spersonalizowany plan nauki na cały rok z materiałami edukacyjnymi i roadmapą",
          "Jedno godzinne spotkanie tygodniowo",
          "Nielimitowany kontakt przez cały rok",
          "Miesiąc na wycofanie się (jeżeli spotkania nie spełniają Twoich oczekiwań)",
          "Certyfikat ukończenia mentoringu",
        ],
      }}
    >
      <Fragment slot="flare">
        <div class="radial-flare size-30 top-0 right-0" aria-hidden="true">
        </div>
      </Fragment>
      <Fragment slot="icon">
        <svg
          width="64"
          height="64"
          viewBox="0 0 64 64"
          fill="none"
          aria-labelledby="senior-1-year-icon"
          xmlns="http://www.w3.org/2000/svg"
        >
          <title id="senior-1-year-icon">Ikona rakiety</title>
          <g filter="url(#filter0_d_211_698)">
