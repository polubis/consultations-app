    </PlanTile>

    <PlanTile
      button={{
        label: "Zobacz materiały",
        href: appConfig.contactSection.calendarLink,
        title: "Zobacz dostępne materiały edukacyjne dla planu Samouk",
      }}
      price={{
        id: "mocked-technical-interview-price",
        label: `${appConfig.plansSection.plans.mockedTechInterview.cost} ${appConfig.plansSection.plans.mockedTechInterview.currency}`,
        description: "Bonus (4k tokenów do 4markdown.com)",
      }}
      heading={{
        title: {
          label: appConfig.plansSection.plans.mockedTechInterview.label,
          id: "mocked-technical-interview-title",
        },
        description: {
          label: "Maglujemy wzdłuż i wszerz aby określić Twój poziom",
          id: "mocked-technical-interview-description",
        },
      }}
      benefits={{
        title: `Korzyści planu ${appConfig.plansSection.plans.mockedTechInterview.label}`,
        items: [
          "2 godzinne spotkanie",
          "Profesjonalna rozmowa techniczna",
          "Szczegółowy raport po spotkaniu",
          "Stworzona Roadmapa rozwoju i plan działania",
          "Dostęp do materiałów edukacyjnych",
          "Sprawdzenie holistyczne, od kodowania i wiedzy do umiejętności miękkich",
          "30 minut na wycofanie się (jeżeli spotkanie nie spełnia Twoich oczekiwań)",
        ],
      }}
    >
      <Fragment slot="flare">
        <div
          class="radial-flare size-30 bottom-[40%] left-[50%]"
          aria-hidden="true"
        >
        </div>
      </Fragment>
      <Fragment slot="icon">
        <svg
          width="68"
          height="59"
          viewBox="0 0 68 59"
          fill="none"
          xmlns="http://www.w3.org/2000/svg"
          role="img"
          aria-labelledby="self-study-icon"
        >
          <title id="self-study-icon">Ikona laptopa</title>
          <g filter="url(#filter0_d_211_714)">
            <path
              d="M50.25 35.25H49V20.25C49 19.2554 48.6049 18.3016 47.9017 17.5983C47.1984 16.8951 46.2446 16.5 45.25 16.5H22.75C21.7554 16.5 20.8016 16.8951 20.0983 17.5983C19.3951 18.3016 19 19.2554 19 20.25V35.25H17.75C17.4185 35.25 17.1005 35.3817 16.8661 35.6161C16.6317 35.8505 16.5 36.1685 16.5 36.5V39C16.5 39.9946 16.8951 40.9484 17.5983 41.6517C18.3016 42.3549 19.2554 42.75 20.25 42.75H47.75C48.7446 42.75 49.6984 42.3549 50.4016 41.6517C51.1049 40.9484 51.5 39.9946 51.5 39V36.5C51.5 36.1685 51.3683 35.8505 51.1339 35.6161C50.8995 35.3817 50.5815 35.25 50.25 35.25ZM31.5 20.25H36.5C36.8315 20.25 37.1495 20.3817 37.3839 20.6161C37.6183 20.8505 37.75 21.1685 37.75 21.5C37.75 21.8315 37.6183 22.1495 37.3839 22.3839C37.1495 22.6183 36.8315 22.75 36.5 22.75H31.5C31.1685 22.75 30.8505 22.6183 30.6161 22.3839C30.3817 22.1495 30.25 21.8315 30.25 21.5C30.25 21.1685 30.3817 20.8505 30.6161 20.6161C30.8505 20.3817 31.1685 20.25 31.5 20.25ZM49 39C49 39.3315 48.8683 39.6495 48.6339 39.8839C48.3995 40.1183 48.0815 40.25 47.75 40.25H20.25C19.9185 40.25 19.6005 40.1183 19.3661 39.8839C19.1317 39.6495 19 39.3315 19 39V37.75H49V39Z"
