---
// https://pagespeed.web.dev/analysis/https-consultations-app-production-pages-dev/gv86sq0zh6?form_factor=mobile
import MainLayout from "@/layouts/main.astro";
import { LayoutNavigation } from "@/components/layout/layout-navigation";
import HeroSection from "@/features/home/hero-section.astro";
import CategoriesSection from "@/features/home/categories-section.astro";
import AboutSection from "@/features/home/about-section.astro";
import PlansSection from "@/features/home/plans-section.astro";
import { MeetingSection } from "@/features/home/meeting-section";
import { TestimonialsSection } from "@/features/home/testimonials-section";
import { FaqSection } from "@/features/home/faq-section";
import MaterialsSection from "@/features/home/materials-section.astro";
import Footer from "@/features/home/footer.astro";
---
<MainLayout>
  <LayoutNavigation client:load />
  <main>
    <HeroSection />
    <CategoriesSection />
    <AboutSection />
    <PlansSection />
    <MeetingSection client:visible />
    <TestimonialsSection client:visible />
    <MaterialsSection />
    <FaqSection client:visible />
    <Footer />
  </main>
</MainLayout>
